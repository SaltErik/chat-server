{
  "compilerOptions": {
    "outDir": "./dist/",
    "alwaysStrict": true,
    "target": "esnext",
    "jsx": "react",
    "downlevelIteration": true,
    "importHelpers": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": false,
    "allowJs": false,
    "moduleResolution": "node",
    "resolveJsonModule": true
  }
}
